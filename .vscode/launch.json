{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Main",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "dev.ini"
      ]
    },
    {
      "name": "Wails: Production settings",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/settings/build/bin/settings",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Debug settings",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/settings/build/bin/settings",
      "preLaunchTask": "build debug",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Dev settings",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/settings/build/bin/settings",
      "preLaunchTask": "build dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": []
    },
    {
      "name": "Python: CurDir",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": []
    }
  ]
}